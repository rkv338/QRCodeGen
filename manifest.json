{
    "manifest_version": 3,
    "name": "QR Code Generator",
    "version": "1.0",
    "description": "Generate a QR code for the current website.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "qr.png"
      }
    }
  }